  },
  "author": "Turkhan Badalov @Badalov97",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/turok1997/blender-ui.git"
  },
  "bugs": {
    "url": "https://github.com/turok1997/blender-ui/issues"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-env": "^1.6.1",
    "cross-env": "^5.1.4",
    "cssnano": "^3.10.0",
    "grunt-contrib-jasmine": "^1.1.0",
    "jasmine": "^3.1.0",
    "postcss-cssnext": "^3.1.0",
    "rimraf": "^2.6.2",
    "rollup": "^0.57.1",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-commonjs": "^9.1.0",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-postcss": "^1.6.1",
