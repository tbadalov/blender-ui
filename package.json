{
  "name": "blender-ui",
  "version": "1.0.5",
  "description": "Liteweight library that simulates window partitioning in a browser like in Blender 3D modelling software",
  "keywords": [
    "css"
  ],
  "main": "./dist/blender-ui.js",
  "minified:main": "./dist/blender-ui.min.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean" : "rimraf ./dist/*",
